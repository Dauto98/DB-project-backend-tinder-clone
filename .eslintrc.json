{
  "parser": "babel-eslint",
  "extends": [
    "airbnb-base"
  ],
  "env": {
    "es6": true,
    "node": true
  },
  "rules": {
    "quotes": ["error", "double"],
    "max-len": 0,
    "linebreak-style": 0,
    "no-use-before-define": 0,
    "comma-dangle": 0,
    "eol-last": 0,
    "spaced-comment": 0,
    "no-unused-vars": 1,
    "arrow-body-style": 0,
    "no-else-return": 1,
    "keyword-spacing": 1,
    "arrow-parens": 0,
    "camelcase": 0,
    "no-mixed-operators": 0,
    "no-param-reassign": 0,
    "no-script-url": 0,
    "radix":0,
    "no-case-declarations":0,
    "operator-assignment":0,
    "no-plusplus":0,
    "no-nested-ternary":0,
    "object-curly-newline" : 0,
    "import/extensions" : 0,
    "global-require": 0
  },
  "settings": {
    "import/resolver": "node"
  }
}
